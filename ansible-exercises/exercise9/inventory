all:
  hosts:
    web01:
      ansible_host: 0.0.0.0   # Private IP address 1
    web02:
      ansible_host: 0.0.0.0   # Private IP address 2
    web03:
      ansible_host: 0.0.0.0   # Private IP address 3
      ansible_user: ubuntu
    db01:
      ansible_host: 0.0.0.0   # Private IP address 4

  children:
    webservers:
      hosts:
        web01:
        web02:
        web03:
    dbservers:
      hosts:
        db01:
    dc_oregon:
      children:
        webservers:
        dbservers:
      vars:
        ansible_user: ec2-user
        ansible_ssh_private_key_file: keyname # name.pem

